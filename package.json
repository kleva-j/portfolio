{
  "name": "vite-react-ts-ghactions-template",
  "version": "1.0.0",
  "description": "React + TypeScript + Vite + Github Actions starter template",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pchmn/vite-react-ts-ghactions-template.git"
  },
  "keywords": [
    "react",
    "vite",
    "typescript",
    "template",
    "ghactions",
    "vitest"
  ],
  "author": "pchmn",
  "license": "MIT",
  "homepage": "https://github.com/pchmn/vite-react-ts-ghactions-template",
	"packageManager": "pnpm@8.2.0",
  "scripts": {
    "dev": "run-p dev:* --print-label",
    "start": "vite",
    "build": "tsc && vite build",
		"gh-pages": "build:ci && git subtree push --prefix dist origin gh-pages",
    "build:ci": "tsc && vite build && cp 404.html dist/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage",
    "serve": "vite preview",
    "prepare": "git init && husky install && rm -rf .git/hooks && ln -s ../.husky .git/hooks",
    "lint": "eslint --cache --fix ./src",
    "dev:client": "wait-port 4000 && vite",
    "dev:server": "tsx src/server/index.ts"
  },
  "dependencies": {
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.8",
    "@tanstack/react-query": "^4.16.1",
    "@tanstack/react-query-devtools": "^4.16.1",
    "@tanstack/react-router": "0.0.1-beta.25",
    "@tanstack/react-router-devtools": "0.0.1-beta.19",
    "@trpc/client": "^10.1.0",
    "@trpc/react-query": "^10.1.0",
    "@trpc/server": "^10.1.0",
    "autoprefixer": "^10.4.13",
    "axios": "^1.2.0",
    "postcss": "^8.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.4",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/prompt-cli": "^17.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/node": "^18.6.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vitejs/plugin-react": "^2.0.0",
    "c8": "^7.12.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.6.0",
    "happy-dom": "^6.0.4",
    "husky": "^8.0.1",
    "lint-staged": "^11.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "tsx": "^3.12.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.21.0",
    "wait-port": "^1.0.4"
  }
}
